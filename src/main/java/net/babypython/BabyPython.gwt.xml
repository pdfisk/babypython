<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.10.0//EN"
  "http://www.gwtproject.org/doctype/2.10.0/gwt-module.dtd">
<module rename-to='babypython'>

  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.json.JSON" />
  <inherits name="org.antlr.v4.antlr4-runtime" />
  <inherits name="org.gwtproject.nio.GwtNioSupport" />
  <inherits name="elemental2.core.Core" />
  <inherits name="elemental2.dom.Dom" />
  <inherits name="elemental2.indexeddb.IndexedDb" />
  <inherits name="elemental2.webstorage.WebStorage" />
  <inherits name='com.google.web.bindery.event.EventBinder'/>

  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <entry-point class='net.babypython.client.Main'/>

  <source path='client'/>
</module>
